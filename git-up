#!/bin/bash

echo "=============================="
echo "Agregando todos los cambios"
echo "=============================="

git add .

echo -e "\033[37;44m Agregar Commit: \033[0m"
read varcommit
git commit -m "$varcommit 4-git-up"


echo -e "\033[30;42m Branch to Push: \033[0m"
read varbranch
git push -u origin $varbranch
