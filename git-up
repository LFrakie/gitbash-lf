#!/bin/bash

echo "=============================="
echo "Agregando todos los cambios"
echo "=============================="

git add .

echo -e "\e[0;32mAgregar Commit: \e[0m"
read varcommit
git commit -m "$varcommit From git-up"


echo "Branch to Push: "
read varbranch
git push -u origin $varbranch 
